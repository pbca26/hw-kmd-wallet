    "babel-polyfill": "^6.26.0",
    "bip32-path": "^0.4.2",
    "bitcoinjs-lib": "^4.0.2",
    "bs58check": "^2.1.2",
    "bulma": "^0.7.2",
    "coinselect": "github:bitcoinjs/coinselect",
    "compare-versions": "^3.6.0",
    "date-fns": "^1.30.1",
    "lodash": "^4.17.19",
